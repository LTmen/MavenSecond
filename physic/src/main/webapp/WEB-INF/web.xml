 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  
  	<servlet>
		<servlet-name>UserServlet</servlet-name>
		<servlet-class>com.lt.physic.servlet.basicinfojServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>SaveUserServlet</servlet-name>
		<servlet-class>com.lt.physic.servlet.SaveUserServlet</servlet-class>
	</servlet>
  	<servlet-mapping>
		<servlet-name>UserServlet</servlet-name>
		<url-pattern>/get_users.php</url-pattern>
	</servlet-mapping>
	
  	<servlet-mapping>
		<servlet-name>SaveUserServlet</servlet-name>
		<url-pattern>/save_user.php</url-pattern>
	</servlet-mapping>
</web-app>
